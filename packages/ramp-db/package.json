{
  "description": "Prisma client and generated models for a database corresponding to Ramp API objects",
  "exports": {
    "./*": "./dist/index.js"
  },
  "license": "Apache-2.0",
  "name": "@repo/ramp-db",
  "private": true,
  "scripts": {
    "build": "tsc",
    "ramp-db:deploy": "prisma migrate deploy",
    "ramp-db:generate" : "prisma generate",
    "ramp-db:migrate": "prisma migrate dev --skip-generate"
  },
  "version": "0.0.0",
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "prisma": "catalog:prisma6"
  },
  "dependencies": {
    "@prisma/client": "catalog:prisma6"
  }
}
